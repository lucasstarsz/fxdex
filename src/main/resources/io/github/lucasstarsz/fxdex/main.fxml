<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.lucasstarsz.fxdex.vmodel.DexViewModel">
   <children>
      <MenuBar fx:id="menuBar" nodeOrientation="RIGHT_TO_LEFT">
        <menus>
          <Menu fx:id="dexMenu" mnemonicParsing="false" text="Pokedex" />
        </menus>
      </MenuBar>
      <ScrollPane hbarPolicy="NEVER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" pannable="true" prefHeight="400.0" prefWidth="600.0" vbarPolicy="ALWAYS">
         <content>
            <VBox fx:id="dexContainer" prefHeight="200.0" prefWidth="100.0" />
         </content>
      </ScrollPane>
   </children>
</VBox>
